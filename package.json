    "test": "mocha-webpack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --require resources/assets/js/tests/setup.js resources/assets/js/tests/\\*\\*/\\*.spec.js",
    "test-watch": "mocha-webpack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --watch --require resources/assets/js/tests/setup.js resources/assets/js/tests/\\*\\*/\\*.spec.js"
